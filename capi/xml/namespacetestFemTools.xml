      <memberdef kind="function" id="namespacetestFemTools_1a1fede00c3d9a64b3bc848f9071737012" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>def</type>
        <definition>def testFemTools.getEdgeNodesInPhysicalSpace</definition>
        <argsstring>(p0, p1, order)</argsstring>
        <name>getEdgeNodesInPhysicalSpace</name>
        <param>
          <type>p0</type>
          <defname>p0</defname>
        </param>
        <param>
          <type>p1</type>
          <defname>p1</defname>
        </param>
        <param>
          <type>order</type>
          <defname>order</defname>
        </param>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para><verbatim>get Lagrange nodes for P^{order} assuming parameterization between
2d nodes p0 and p1 ,
  p = p0*t + (1-t)*p1

return is array that&apos;s (k+1) x 3
</verbatim> </para>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
